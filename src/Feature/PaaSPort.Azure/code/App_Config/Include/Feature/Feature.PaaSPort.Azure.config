<configuration xmlns:patch="http://www.sitecore.net/xmlconfig/" xmlns:set="http://www.sitecore.net/xmlconfig/set/">
  <sitecore>
    <settings>
      <!-- SETTING: PAASPORT PIPELINE GROUP NAME
            
            Defines the group-->
      <setting name="PaaSPort.PipelineGroupName" value="PaaSPort" />
    </settings>
    <pipelines>
      <group groupName="PaaSPort" name="PaaSPort">
        <pipelines>
          <getResourceGroups
            help="Pipelines should inherit from TheInjectables.Feature.PaaSPort.DXF.Pipelines.GetResourceGroups.BaseGetResourceGroupsProcessor">
            <processor
              type="TheInjectables.Feature.PaaSPort.Azure.Pipelines.GetResourceGroups.GetResourceGroupsProcessor, TheInjectables.Feature.PaaSPort.Azure" />
          </getResourceGroups>
        </pipelines>
      </group>
    </pipelines>
    <paasport>
      <azure>
        <serviceManager
          type="TheInjectables.Feature.PaaSPort.Azure.Client.AzureServiceManager, TheInjectables.Feature.PaaSPort.Azure"
          singleInstance="true" />
        <servicePipelines>
          <getResourceGroups>
            <servicePipelineArgs
              type="TheInjectables.Feature.PaaSPort.Azure.Pipelines.GetResourceGroups.GetResourceGroupsArgs, TheInjectables.Feature.PaaSPort.Azure" />
          </getResourceGroups>
        </servicePipelines>
      </azure>
    </paasport>
  </sitecore>
</configuration>